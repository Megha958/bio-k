{{ 'megamenu-header.css' | asset_url | stylesheet_tag }}
<div class="navbar navabar-desktop">
  <nav class="navbar">
    <ul class="navbar_list">
      {% for menu_item in menu.links %}
      <li class="navbar_item dropdown">
        
        {% assign images_block = false %} {% assign images_block_with_product =
        false %} {% assign submenu_block = false %} {% for block in
        section.blocks %} {% assign menu_title = menu_item.title | handleize %}
        {% assign menu_block_title = block.settings.menu | handleize %} {% if
        menu_block_title == menu_title %} 
        {% if block.type == 'promotion-images'
        %} {% assign images_block = true %} 
        
        {% if menu_item.title != blank %}
        <a href="{{ menu_item.url }}" class="navbar_link dropdown_toggle"
          >{{ menu_item.title }}
          <span class="chevron">
            <svg
              width="12"
              height="12"
              viewBox="0 0 13 12"
              fill="none"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path
                d="M12.1367 3.375L6.88672 8.625L1.63672 3.375"
                stroke="currentColor"
                stroke-width="1.5"
                stroke-linecap="round"
                stroke-linejoin="round"
              />
            </svg>
          </span>
        </a>
        <div class="dropdown_menu">
          <div class="">
            <div class="">
              <div class="dropdown_menu-inner animate__animated animate__fadeInDown">
                <div class="megamenu-first-row">
                  <div class="megamenu-row-inner-first">
                    <div class="header-list-megamenu">
                      <div class="header-main-list-megamenu">
                        <div class="header-list-megamenu-inner">
                          <span class="header-header-text">{{ block.settings.menu_heading }}</span>
                          <div class="header-header-text-list">
                            <ul class="cust-inner-menu">
                              {% for link in linklists[block.settings.inner_menu].links %}
                                <li>
                                  <a href="{{ link.url }}">{{ link.title }}</a>
                                </li>
                              {% endfor %}
                            </ul>
                          </div>
                        </div>

                        
                      </div>
                    </div>
                  </div>
                  <div class="megamenu-row-inner-second">
                    <div class="header-list-megamenu">
                        <div class="header-list-megamenu-inner">
                        <span class="header-header-text">{{ block.settings.menu_heading_2 }}</span>
                        <div class="header-header-text-list">
                        <ul class="cust-inner-menu">
                        {% for link in linklists[block.settings.inner_menu_2].links %}
                            <li>
                            <a href="{{ link.url }}">{{ link.title }}</a>
                            </li>
                        {% endfor %}
                        </ul>
                        </div>
                        </div>
                    </div>
                  </div>
                </div>
                <div class="megamenu-second-row">
                  <div class="megamenu-image-with-text-inner">
                    <div class="image-width-text-header">
                      <div class="product-360-category-main">
                          <a href="{{ block.settings.cta_link_1 }}" class="product-360-category-inner">
                            <div class="product-360-image">
                              <div class="image">
                                <img
                                  src="{{ block.settings.desktop_image_1 | img_url: 'master' }}"
                                  alt="Product Image"
                                  class="desktop-img"
                                />
                                <img
                                  src="{{ block.settings.mobile_image_1 | img_url: 'master' }}"
                                  alt="Product Image"
                                  class="mobile-img"
                                />
                              </div>
                            </div>
                            <div class="product-360-text-area">
                              <div class="product-360-header-main">
                                <div class="common-h5 product-360-header">
                                  <h5>{{ block.settings.header_text_1 }}</h5>
                                </div>
                                <div class="product-360-para">
                                  <p>{{ block.settings.paragraph_text_1 }}</p>
                                </div>
                              </div>
                              <div class="link makro--link product-360-link ">
                                {{ block.settings.cta_text_1 }} 
                                {% render 'arrow-svg' %}
                              </div>
                            </div>
                          </a>
                        </div>

                    </div>

                    <div class="image-width-text-header">
                      <div class="product-360-category-main">
                          <a href="{{ block.settings.cta_link_2 }}" class="product-360-category-inner">
                            <div class="product-360-image">
                              <div class="image">
                                <img
                                  src="{{ block.settings.desktop_image_2 | img_url: 'master' }}"
                                  alt="Product Image"
                                  class="desktop-img"
                                />
                                <img
                                  src="{{ block.settings.mobile_image_2 | img_url: 'master' }}"
                                  alt="Product Image"
                                  class="mobile-img"
                                />
                              </div>
                            </div>
                            <div class="product-360-text-area">
                              <div class="product-360-header-main">
                                <div class="common-h5 product-360-header">
                                  <h5>{{ block.settings.header_text_2 }}</h5>
                                </div>
                                <div class="product-360-para">
                                  <p>{{ block.settings.paragraph_text_2 }}</p>
                                </div>
                              </div>
                              <div class="link makro--link product-360-link">
                                {{ block.settings.cta_text_2 }} 
                                {% render 'arrow-svg' %}
                              </div>
                            </div>
                          </a>
                        </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        {% else %}
        <a href="{{ menu_item.url }}">{{ menu_item.title }}
         
        </a>
        {% endif %} 
        {% elsif block.type == 'about' %} 
        {% assign
        images_block_with_product = true %}
         {% if menu_item.links != blank and
        images_block_with_product == true %}
